  "devDependencies": {
    "del": "^3.0.0",
    "eslint": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-css-selector-limit": "^0.1.2",
    "gulp-htmlclean": "^2.7.14",
